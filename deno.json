{
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 200,
    "semiColons": true,
    "singleQuote": true
  },
  "test": {
    "include": [
      "file/**/*.spec.ts",
      "file/**/*.test.ts"
    ]
  },
  "tasks": {
    "start-temporary": "deno run -A --watch=file/interface/fresh/static/,file/interface/fresh/routes/ ./file/interface/fresh/dev.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.2",
    "@std/testing": "jsr:@std/testing@1.0.0",
    "file/": "./file/",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "shared/": "./shared/"
  }
}
